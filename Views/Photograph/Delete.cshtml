@model Digital_photos.Photograph

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/Backend_Layout.cshtml";
}



<div id="layoutSidenav_content">
    <main>
        <div class="container-fluid px-4">
            <h2 class="mt-4">Delete</h2>

            <h2 class="ms-4 mt-2">Photograph</h2>

            <h3>Are you sure you want to delete this?</h3>
            <div>
                <h4>Photograph</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        User Id
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.UserId)
                    </dd>

                    <dt>
                        Photograph
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Photo)
                    </dd>

                    <dt>
                        Category Id
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.category_id)
                    </dd>

                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()


                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-outline-dark" />

                        <a href="~/Photograph/Index">
                            <input type="button" value="Cancel" class="btn btn-outline-dark" />
                        </a>
                    </div>
                }
            </div>
