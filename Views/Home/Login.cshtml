    <script src="~/frontend/assets/plugin/mixItUp/js/jquery.mixitup.js"></script>

    <script type="text/javascript" src="~/frontend/assets/plugin/counter/js/jquery.countTo.js"></script>

    <script type="text/javascript" src="~/frontend/assets/plugin/counter/js/jquery.appear.js"></script>

    <script src="~/frontend/assets/js/main.js"></script>

</body>


