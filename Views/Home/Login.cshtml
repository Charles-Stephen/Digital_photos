<body>

    <!--//================preloader  start==============//-->

    <div class="preloader">

        <div class="itg-loader">

            <figure>

                <img src="~/frontend/assets/img/loader/loder.png" alt="" />

            </figure>

            <h4>Loading</h4>

        </div>

    </div>

    <!--//================preloader  end==============//-->
    <!--//================Error==============//-->

    <section class="special-section">

        <div class="special-style dark-overlay">

            <div class="error-style-1 parallax-style"></div>

        </div>

        <div class="error-content">

            <div class="container">


                        <figure>

                            <a class="navbar-brand" href="~/Home/Index">
                                <div class="theme-heading">
                                    <h3><span class="heading-shape" style="color:white"><strong>MY</strong> - IMAGE</span></h3>
                                </div>
                            </a>

                        </figure>

                        <div class="container-fluid">
                            <h2 class="text-center mt-5 mb-4" style="color: white">LOGIN</h2>

                            <form method="post" class="mt-5" action="~/Home/Login">
                                <div class="form-floating" style="margin-bottom: 1vw;">
                                    <label for="email" class="text-start">Email</label>
                                    <input type="email" id="email" name="email" class="form-control" placeholder="Enter Email Address" required />
                                </div>

                                <div class="form-floating" style="margin-bottom: 1vw;">
                                    <label for="passw">Password</label>
                                    <input type="password" id="passw" name="pass" class="form-control" placeholder="Enter Password" required />
                                </div>

                                <div class="form-floating" style="margin-bottom: 1vw;">
                                    <input type="submit" value="Login" class="itg-button" placeholder="Login" />
                                    <p class="text-danger">@ViewBag.emailerror</p>
                                    <small>Do not have an Account? <a class="text-primary" href="~/Home/Registration"> CREATE ACCOUNT</a></small>
                                </div>
                            </form>
                        </div>

                        <div align="center" class="w-50">

                            <a href="~/Home/Index" class="itg-button">go home</a>

                            <a href="~/Home/Contact" class="itg-button">contact</a>

                        </div>


            </div>

        </div>

    </section>

    <!--//================Error Soon==============//-->
