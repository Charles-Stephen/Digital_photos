@model Photo.user

@{
    ViewBag.Title = "DeleteConfirmed";
    Layout = "~/Views/Shared/Backend_Layout.cshtml";
}


<div id="layoutSidenav_content">
    <main>
        <div class="container-fluid px-4">
            <h2 class="mt-4">DeleteConfirmed</h2>

            <h3>Are you sure you want to delete this?</h3>
            <div>
                <h4 class="ms-4 mt-2">User</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        First Name
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.First_Name)
                    </dd>

                    <dt>
                        Last Name
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Last_Name)
                    </dd>

                    <dt>
                        Email
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Email)
                    </dd>

                    <dt>
                        Password
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Pass)
                    </dd>

                    <dt>
                        User Type
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.User_Type)
                    </dd>

                    <dt>
                        User Profile
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.User_Profile)
                    </dd>

                    <dt>
                        Date Of Birth
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Date_Of_Birth)
                    </dd>

                    <dt>
                        Gender
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Gender)
                    </dd>

                    <dt>
                        Phone
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Phone)
                    </dd>

                    <dt>
                        Order Id
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Order_id)
                    </dd>

                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Delete" class="btn btn-outline-dark" />

                        <a href="~/Contact/Index">
                            <input type="button" value="Cancel" class="btn btn-outline-dark" />
                        </a>
                    </div>
                }
            </div>
        </div>
    </main>